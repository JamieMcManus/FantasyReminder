{
  "name": "Fantasy Football Reminder",
  "version": "1.1",
  "description":
    "This extension will remind you to set your fantasy team",
 
  "permissions": [
    "notifications"
  ],
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
 "browser_action":{
        "default_icon":"icon.png"
    },
  "background": { "scripts": ["background.js"] },
  "manifest_version": 2,


  "web_accessible_resources": [
    "icon.png"
  ]
}
